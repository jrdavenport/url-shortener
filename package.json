{
  "name": "url-shortener",
  "version": "1.0.0",
  "description": "A collection of Docker containers which shortens a URL for easier sharing",
  "main": "index.js",
  "scripts": {
    "dev:install": "docker-compose -f docker-compose.dev.yml build",
    "dev:up": "docker-compose -f docker-compose.dev.yml up",
    "dev:down": "docker-compose -f docker-compose.dev.yml down",
    "dev:uninstall": "docker-compose -f docker-compose.dev.yml down --rmi all"
  },
  "author": "James Davenport",
  "license": "ISC"
}
